{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "pictures",
  "info": {
    "name": "Picture"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "collection": {
      "model": "collection",
      "via": "pictures"
    },
    "Format": {
      "type": "enumeration",
      "enum": [
        "Square",
        "Portrait",
        "Landscape"
      ]
    },
    "PriceSize": {
      "type": "component",
      "repeatable": false,
      "component": "pictures.price-and-size"
    }
  }
}
